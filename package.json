		"date-fns": "^2.30.0",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-seo": "^1.5.4",
		"tslib": "^2.6.2",
